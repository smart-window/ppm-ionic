$('#license-manager').submit(function(e){
  e.preventDefault();
  $('.button').addClass('busy');
  $('.button').addClass('loading');

  $.ajax({
    url: "https://premiumdatingscript.com/clients/reset-license-domains.php", 
    data: {
      license: $('#license').val(),
      oldDomain: $('#old-domain').val(),
      newDomain: $('#new-domain').val()
    },
    type:'POST',
    dataType: 'JSON',
    success: function(result){
      console.log(result);
      $('.button').removeClass('busy');
      $('.button').removeClass('loading');       
      alert(result.result);
      window.location.reload();
    }
  });  
});

$('#add-domain').submit(function(e){
  e.preventDefault();
  $('.button').addClass('busy');
  $('.button').addClass('loading');

  if( $('#license').val() == '' || $('#increaser').val() == ''){
    $('#add-domain-error').fadeIn();
    $('#add-domain-error').html('Please fill all the inputs');
    setTimeout(function(){
      $('.button').removeClass('busy');
      $('.button').removeClass('loading');  
    },500);
    return false;
  }

  $.ajax({
    url: "https://premiumdatingscript.com/clients/add-domain.php", 
    data: {
      licenseBelloo: $('#license').val(),
      licenseIncrease: $('#increaser').val(),
    },
    type:'POST',
    dataType: 'JSON',
    success: function(result){
      $('.button').removeClass('busy');
      $('.button').removeClass('loading');       
      alert('License Updated Successfully');
      window.location.reload();
    },
    error: function(result){
      $('.button').removeClass('busy');
      $('.button').removeClass('loading');       
      alert('Waiting for Envato Approval');
      window.location.reload();
    }    
  });  
})

